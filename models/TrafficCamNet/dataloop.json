{
  "name": "traffic-cam-net",
  "description": "An app to install the TrafficCamNet model. The model described detects one or more physical objects from four categories within an image and returns a box around each object, as well as a category label for each object. The four categories of objects detected by this model are \u00e2\u20ac\u201c car, persons, road signs and bicycles.",
  "attributes": {
    "Provider": "Nvidia",
    "License": "EULA",
    "Category": "Model",
    "Computer Vision": "Object Detection"
  },
  "displayName": "TrafficCamNet",
  "version": "v0.1.5",
  "scope": "public",
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/nvidia-models.git",
    "gitTag": "v0.1.5"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "trafficcamnet-deploy",
        "versions": {
          "dtlpy": "1.89.6"
        },
        "runtime": {
          "podType": "gpu-t4",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/gpu/nvidia-tao:0.1.2",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 1000
          },
          "preemptible": false
        },
        "secrets": []
      }
    ],
    "modules": [
      {
        "name": "trafficcamnet-module",
        "entryPoint": "models/model_adapter.py",
        "className": "TaoModelAdapter",
        "computeConfig": "trafficcamnet-deploy",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          },
          {
            "type": "String",
            "name": "ngc_api_key_secret_name"
          },
          {
            "type": "String",
            "name": "ngc_org_secret_name"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The input images for prediction."
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotations[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "The inference function of the model."
          }
        ]
      }
    ],
    "models": [
      {
        "name": "nv-traffic-cam-net-model",
        "moduleName": "trafficcamnet-module",
        "scope": "project",
        "status": "trained",
        "configuration": {
          "model_name": "traffic-cam-net",
          "model_config": {}
        },
        "inputType": "image",
        "outputType": "box",
        "labels": [
          "car",
          "bicycle",
          "person",
          "road_sign"
        ],
        "metadata": {},
        "description": "The model described detects one or more physical objects from four categories within an image and returns a box around each object, as well as a category label for each object. The four categories of objects detected by this model are \u00e2\u20ac\u201c car, persons, road signs and bicycles."
      }
    ]
  }
}