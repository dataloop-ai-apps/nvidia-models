{
  "name": "lpr-net",
  "description": "An app to install the License Plate Detection (LPDNet) model. The model is license plate recognition network, which aims to recognize characters in license plates from cropped RGB license plate images.",
  "attributes": {
    "Hub": [
      "Nvidia",
      "Dataloop"
    ],
    "Category": "Model",
    "Provider": "NVIDIA",
    "Media Type": "Image",
    "Computer Vision": "Object Detection",
    "License": "EULA"
  },
  "displayName": "LicensePlateRecognition",
  "version": "0.2.5",
  "scope": "public",
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/nvidia-models.git",
    "gitTag": "0.2.5"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "lpr-net-deploy",
        "runtime": {
          "podType": "gpu-t4",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/gpu/nvidia-tao:0.1.2",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 1000
          },
          "preemptible": false
        },
        "secrets": []
      }
    ],
    "modules": [
      {
        "name": "lpr-net-module",
        "entryPoint": "models/model_adapter.py",
        "className": "TaoModelAdapter",
        "computeConfig": "lpr-net-deploy",
        "initInputs": [
          {
            "type": "String",
            "name": "ngc_api_key_secret_name"
          },
          {
            "type": "String",
            "name": "ngc_org_secret_name"
          },
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The input images for prediction."
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "The inference function of the model."
          },
          {
            "name": "predict_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "The input dataset of the items required for prediction."
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "The DQL in json format to get all the items required for prediction."
              }
            ],
            "output": [],
            "displayName": "Predict Dataset",
            "displayIcon": "",
            "description": "Inference function of the model on a dataset."
          },
          {
            "name": "evaluate_model",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Dataloop Filter DQL"
              }
            ],
            "output": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              }
            ],
            "displayName": "Evaluate a Model",
            "displayIcon": "",
            "description": "Function to evaluate Nvidia TAO model performance"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "nv-lpr-net-model",
        "moduleName": "lpr-net-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "model_name": "lpr-net",
          "model_config": {}
        },
        "inputType": "image",
        "outputType": "note",
        "labels": [],
        "metadata": {},
        "description": "The model is license plate recognition network, which aims to recognize characters in license plates from cropped RGB license plate images."
      }
    ]
  }
}