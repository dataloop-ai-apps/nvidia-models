{
  "name": "facedetectir-app",
  "description": "An app to install the FaceDetectIR model. The model described in this card detects one or more faces in the given image / video. Compared to the PeopleNet model, this model gives better results detecting large faces, such as faces in webcam images.",
  "categories": [],
  "displayName": "facedetectir App",
  "version": "v0.1.0",
  "scope": "project",
  "codebaseZ": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/nvidia-models.git",
    "gitTag": "v0.1.0"
  },
  "components": {
    "facedetectir-deploy": {
      "type": "computeConfig",
      "spec": {
        "name": "facedetectir-deploy",
        "runtime": {
          "podType": "gpu-t4",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/gpu/nvidia-tao:0.1.2",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 1000
          },
          "preemptible": false
        }
      }
    },
    "facedetectir-module": {
      "type": "module",
      "spec": {
        "name": "facedetectir-module",
        "entryPoint": "facedetectir.py",
        "className": "FaceDetectIR",
        "computeConfig": "facedetectir-deploy",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The input images for prediction."
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotations[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "The inference function of the model."
          }
        ]
      }
    },
    "facedetectir": {
      "type": "model",
      "spec": {
        "name": "facedetectir",
        "moduleName": "facedetectir-module",
        "scope": "project",
        "status": "trained",
        "configuration": {
          "model_name": "face-detect-ir",
          "model_config": {}
        },
        "inputType": "image",
        "outputType": "box",
        "metadata": {},
        "description": "The model described in this card detects one or more faces in the given image / video. Compared to the PeopleNet model, this model gives better results detecting large faces, such as faces in webcam images."
      }
    }
  }
}