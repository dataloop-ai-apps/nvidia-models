{
    "name": "facedetectir-app",
    "description": "An app to install the FaceDetectIR model. The model detects one or more faces in the given image / video. Compared to the PeopleNet model, this model gives better results detecting large faces, such as faces in webcam images.",
    "attributes": {
        "provider": "Nvidia",
        "license": "EULA",
        "category": "Model",
        "Computer Vision": "Object Detection"
    },
    "displayName": "facedetectir App",
    "version": "v0.1.1",
    "scope": "project",
    "codebaseZ": {
        "type": "git",
        "gitUrl": "https://github.com/dataloop-ai-apps/nvidia-models.git",
        "gitTag": "v0.1.1"
    },
    "codebase": {
        "type": "local",
        "src_path": "../.."
    },
    "components": {
        "computeConfigs": [
            {
                "name": "facedetectir-deploy",
                "versions": {
                    "dtlpy": "1.89.6"
                },
                "runtime": {
                    "podType": "gpu-t4",
                    "concurrency": 1,
                    "runnerImage": "gcr.io/viewo-g/piper/agent/runner/gpu/nvidia-tao:0.1.2",
                    "autoscaler": {
                        "type": "rabbitmq",
                        "minReplicas": 0,
                        "maxReplicas": 2,
                        "queueLength": 1000
                    },
                    "preemptible": false
                },
                "secrets": []
            }
        ],
        "modules": [
            {
                "name": "facedetectir-module",
                "entryPoint": "models/model_adapter.py",
                "className": "TaoModelAdapter",
                "computeConfig": "facedetectir-deploy",
                "initInputs": [
                    {
                        "type": "String",
                        "name": "ngc_api_key_secret_name"
                    },
                    {
                        "type": "String",
                        "name": "ngc_org_secret_name"
                    },
                    {
                        "type": "Model",
                        "name": "model_entity"
                    }
                ],
                "functions": [
                    {
                        "name": "predict_items",
                        "input": [
                            {
                                "type": "Item[]",
                                "name": "items",
                                "description": "The input images for prediction."
                            }
                        ],
                        "output": [
                            {
                                "type": "Item[]",
                                "name": "items",
                                "description": "The same input images for prediction."
                            },
                            {
                                "type": "Annotations[]",
                                "name": "annotations",
                                "description": "The predicted annotations."
                            }
                        ],
                        "displayName": "Predict Items",
                        "displayIcon": "",
                        "description": "The inference function of the model."
                    }
                ]
            }
        ],
        "models": [
            {
                "name": "nv-face-detect-ir-model",
                "moduleName": "facedetectir-module",
                "scope": "project",
                "status": "trained",
                "configuration": {
                    "model_name": "face-detect-ir",
                    "model_config": {}
                },
                "inputType": "image",
                "outputType": "box",
                "labels": [
                    "face"
                ],
                "metadata": {},
                "description": "The model detects one or more faces in the given image / video. Compared to the PeopleNet model, this model gives better results detecting large faces, such as faces in webcam images."
            }
        ]
    }
}