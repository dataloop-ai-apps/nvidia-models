{
  "name": "peoplenet-app",
  "categories": [],
  "displayName": "peoplenet App",
  "version": "v0.1.0",
  "scope": "project",
  "codebaseZ": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/nvidia-models.git",
    "gitTag": "v0.1.0"
  },
  "components": {
    "peoplenet-deploy": {
      "type": "computeConfig",
      "spec": {
        "name": "peoplenet-deploy",
        "runtime": {
          "podType": "gpu-t4",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/gpu/nvidia-tao:0.1.2",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 1000
          },
          "preemptible": false
        }
      }
    },
    "peoplenet-module": {
      "type": "module",
      "spec": {
        "name": "peoplenet-module",
        "entryPoint": "peoplenet.py",
        "className": "PeopleNet",
        "computeConfig": "peoplenet-deploy",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items"
              }
            ],
            "output": [],
            "displayName": "Predict Items",
            "displayIcon": ""
          }
        ]
      }
    },
    "peoplenet": {
      "type": "model",
      "spec": {
        "name": "peoplenet",
        "moduleName": "peoplenet-module",
        "scope": "project",
        "status": "trained",
        "configuration": {
          "model_name": "people-net",
          "model_config": {}
        },
        "inputType": "image",
        "outputType": "box",
        "metadata": {}
      }
    }
  }
}