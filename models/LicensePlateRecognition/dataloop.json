{
  "name": "lpr-net-app",
  "description": "An app to install the License Plate Detection (LPDNet) model. The model is license plate recognition network, which aims to recognize characters in license plates from cropped RGB license plate images.",
  "attributes": {
    "provider": "Nvidia",
    "license": "EULA",
    "category": "Model",
    "Computer Vision": "Object Detection"
  },
  "displayName": "lpr-net App",
  "version": "v0.1.0",
  "scope": "project",
  "codebaseZ": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/nvidia-models.git",
    "gitTag": "v0.1.0"
  },
  "components": {
    "lpr-net-deploy": {
      "type": "computeConfig",
      "spec": {
        "name": "lpr-net-deploy",
        "versions": {
          "dtlpy": "1.89.6"
        },
        "runtime": {
          "podType": "gpu-t4",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/gpu/nvidia-tao:0.1.2",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 1000
          },
          "preemptible": false
        }
      }
    },
    "lpr-net-module": {
      "type": "module",
      "spec": {
        "name": "lpr-net-module",
        "entryPoint": "license_plate_recognition.py",
        "className": "LPRNet",
        "computeConfig": "lpr-net-deploy",
        "initInputs": [
          {
            "type": "String",
            "name": "ngc_api_key_secret_name"
          },
          {
            "type": "String",
            "name": "ngc_org_secret_name"
          },
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The input images for prediction."
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotations[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "The inference function of the model."
          }
        ]
      }
    },
    "lpr-net": {
      "type": "model",
      "spec": {
        "name": "nv-lpr-net-model",
        "moduleName": "lpr-net-module",
        "scope": "project",
        "status": "trained",
        "configuration": {
          "model_name": "lpr-net",
          "model_config": {}
        },
        "inputType": "image",
        "outputType": "note",
        "metadata": {},
        "description": "The model is license plate recognition network, which aims to recognize characters in license plates from cropped RGB license plate images."
      }
    }
  }
}